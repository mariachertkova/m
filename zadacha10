#include <iostream>
#include <stdarg.h>
#include <locale.h>

using namespace std;

int main (void){
	setlocale (LC_ALL, "Russian");
	int m;//Кол-во минут
	int b=1;//Кол-во бактерий
	int time=0;
	cout<<"Введите кол-во минут m: ";
	cin>>m;
	while (time<m){
		++time;
		b*=2;
	}
	cout<<b<<endl;
	system ("pause");
	return 0;
}
